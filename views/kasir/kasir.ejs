<%- include('../partial/menu.ejs') %>

<table border="1">
    <thead>
        <th>No Meja</th>
        <th>Menu</th>
        <th>Quantity</th>
        <th>Total</th>
        <th>Action</th>
    </thead>
    <tbody>
        <% for(let i=0; i<data.length; i++) {%>
        <tr>
            <td><%= data[i].mejaId %></td>
            <td>
                <ul>
                    <% for(let j=0; j<data[i].Menus.length; j++){%>
                    <li><%= data[i].Menus[j].name%></li>
                    <% } %>
                </ul>
            </td>
            <td>
                <ul>
                    <% for(let k=0; k<data[i].Menus.length; k++){%>
                    <li><%= data[i].Menus[k].PesananMenu.quantity %></li>
                    <% } %>
                </ul>
            </td>
            <td>
                1500000
            </td>
            <td><%= data[i].status %></td>
        </tr>
        <% } %>
    </tbody>
</table>